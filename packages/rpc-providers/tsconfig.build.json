{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "declaration": true,
    "declarationMap": true,
    "incremental": true,
    "composite": true
  },
  "include": ["./src", "package.json", "./contracts", "src/**/*.json"],
  "files": [
    "./src/ProviderWithFallback.ts",
    "./src/ProviderWithFallbackBase.ts"
  ]
}
