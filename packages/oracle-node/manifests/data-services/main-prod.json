{
  "interval": 10000,
  "priceAggregator": "median",
  "defaultSource": ["coingecko"],
  "sourceTimeout": 7000,
  "minValidSourcesPercentage": 40,
  "deviationCheck": {
    "deviationWithRecentValues": {
      "maxPercent": 30,
      "maxDelayMilliseconds": 300000
    }
  },
  "tokens": {
    "SWETH": {
      "source": ["coingecko", "coinmarketcap", "balancer-bb-a-weth"]
    },
    "VST": {
      "source": ["curve-frax-multi-block"]
    },

    "BB-A-WETH": {
      "source": ["coingecko", "coinmarketcap"],
      "skipSigning": true
    },
    "USDC": {
      "source": [
        "binanceus",
        "bitfinex2",
        "bittrex",
        "btcturk",
        "cex",
        "coingecko",
        "coinmarketcap",
        "currencycom",
        "huobipro",
        "kaiko",
        "kraken",
        "kucoin",
        "lbank",
        "okx",
        "probit",
        "sushiswap",
        "uniswap"
      ],
      "skipSigning": true
    },
    "USDT": {
      "source": [
        "binanceus",
        "bitfinex2",
        "bittrex",
        "cex",
        "coinbaseprime",
        "coinbasepro",
        "coingecko",
        "coinmarketcap",
        "currencycom",
        "kaiko",
        "kraken",
        "uniswap"
      ],
      "skipSigning": true
    },
    "FXS": {
